#!/bin/sh


if [ ! -x /bin/busybox-static ]; then
  echo "BUSYBOX not found" >> /tmp/screenshots.log
  exit 0
fi


busybox-static httpd -c /etc/httpd2.conf -p 8081 -h /var/www &


